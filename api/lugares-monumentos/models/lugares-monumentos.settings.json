{
  "kind": "collectionType",
  "collectionName": "lugares_monumentos",
  "info": {
    "name": "lugares-monumentos",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "lnombremonumento": {
      "type": "string",
      "required": true,
      "maxLength": 100
    },
    "ltipomonumento": {
      "type": "enumeration",
